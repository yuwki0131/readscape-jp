# 環境設定
COMPOSE_PROJECT_NAME=readscape
ENVIRONMENT=development

# データベース設定
POSTGRES_DB=readscape
POSTGRES_USER=readscape_user
POSTGRES_PASSWORD=readscape_pass
DB_HOST=postgres
DB_PORT=5432

# JWT設定
JWT_SECRET=docker-development-jwt-secret-key-256-bits-long-for-secure-authentication

# Redis設定
REDIS_PASSWORD=""

# ポート設定
CONSUMER_API_PORT=8080
INVENTORY_API_PORT=8081
POSTGRES_PORT=5432
REDIS_PORT=6379
PGADMIN_PORT=8082

# ログレベル
LOG_LEVEL=INFO